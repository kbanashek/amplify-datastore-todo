{
  "name": "amplify-datastore-todo",
  "main": "./entry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "npm run build:coverage-scripts && jest --coverage && node scripts/format-coverage.js",
    "test:coverage:html": "npm run build:coverage-scripts && jest --coverage && node scripts/generate-coverage-html.js && open coverage/coverage-interactive.html",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "build:coverage-scripts": "tsc scripts/format-coverage.ts scripts/generate-coverage-html.ts --outDir scripts --target ES2020 --module commonjs --esModuleInterop --skipLibCheck --resolveJsonModule",
    "load-aws-credentials": "./scripts/load-aws-credentials.sh",
    "test:translation": "npx tsx scripts/test-translation.ts"
  },
  "dependencies": {
